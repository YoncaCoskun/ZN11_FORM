<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Position Selection"
		class="sapUiPopupWithPadding"
		items="{/ProductCollection}"
		search="_handleDelegatedPositionValueHelpSearch"
		confirm="_handleDelegatedPositionValueHelpClose"
		cancel="_handleDelegatedPositionValueHelpClose">
		<StandardListItem
			iconInset="false"
			title="{Name}"
			description="{ProductId}" />
	</SelectDialog>
</core:FragmentDefinition>